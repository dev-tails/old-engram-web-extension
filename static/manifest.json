{
  "name": "Send to engram",
  "description": "Quickly send websites or text selection to engram",
  "version": "1.3",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/engram-web-extension.js"
  },
  "action": {
    "default_title": "Send to engram",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "permissions": ["contextMenus", "tabs", "notifications"],
  "host_permissions": ["https://engram.xyzdigital.com/"]
}
